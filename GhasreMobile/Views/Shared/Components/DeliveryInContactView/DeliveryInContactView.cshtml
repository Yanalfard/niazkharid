@model DeliveryVm
<form id="ajaxresultDeliveryInContact" class="request mat-card" asp-action="Delivery" method="post" asp-area=""
      data-ajax="true"
      data-ajax-method="POST"
      data-ajax-mode="replace"
      data-ajax-complete="endDeliveryInContact()"
      data-ajax-update="#ajaxresultDeliveryInContact">
    <h3>درخواست خدمات درب منزل</h3>
    <p class="text-3">برای درخواست خدمات تعمیراتی و گارانتی درب منزل این فرم را پر کنید </p>
    <div class="fg-col">
        <label for="TelNo" class="fg-label">شماره تلفن</label>
        <input name="TellNo" type="tel" class="entry" asp-for="TellNo" />
        <span class="text-danger" asp-validation-for="TellNo"></span>
    </div>
    <div class="fg-col">
        <label for="Name" class="fg-label">نام و نام خانوادگی</label>
        <input name="Name" type="text" class="entry" asp-for="Name" />
        <span class="text-danger" asp-validation-for="Name"></span>
    </div>
    <div class="fg-col">
        <label for="Address" class="fg-label">توضیحات</label>
        <textarea name="Message" class="entry" asp-for="Message"></textarea>
        <span class="text-danger" asp-validation-for="Message"></span>
    </div>
    <div class="fg-col">
        <label for="Address" class="fg-label">آدرس</label>
        <textarea name="Address" class="entry" asp-for="Address"></textarea>
        <span class="text-danger" asp-validation-for="Address"></span>
    </div>
    <div class="rowr">
        <button class="btn btn-primary-gradient" onclick="LoadingRun('#ajaxresultDeliveryInContact')">فرستادن درخواست</button>
    </div>
</form>
<script>
    function endDeliveryInContact() {
        $("input").val("");
        $("textarea").val("");
        UIkit.notification('درخواست شما ثبت شد');
        LoadingEnd();
    }
</script>

